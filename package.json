{
  "name": "frostyflow",
  "version": "1.0.0",
  "description": "Bifrost multi-chain liquid staking platform",
  "main": "index.js",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test:connections": "node scripts/test-connections.js",
    "test:wallet": "node scripts/test-wallet.js",
    "test:testnet": "npm run test:connections && npm run test:wallet",
    "dev:testnet": "REACT_APP_ENVIRONMENT=testnet npm start",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "setup:wallet": "./setup-wallet-extension.sh",
    "setup:testnet": "./setup-testnet.sh && ./setup-wallet-extension.sh",
    "test:full": "./test-full-testnet.sh",
    "test:complete": "npm run test:full && npm run test:e2e"
  },
  "dependencies": {
